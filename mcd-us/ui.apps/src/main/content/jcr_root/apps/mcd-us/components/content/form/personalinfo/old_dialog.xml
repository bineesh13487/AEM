                        fieldLabel="Section Sub Heading"
                        name="./sectionSubHeading"
                        xtype="textfield"/>
                    <firstName
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text for first name field. Default set to &lt;b>First Name *&lt;/b>"
                        fieldLabel="First Name Text"
                        name="./firstName"
                        xtype="textfield"/>
                     <firstNameAriaLabel
