                        jcr:primaryType="cq:Widget"
                        defaultValue="yes"
                        fieldDescription="Please select last name is required or not. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="Last Name Required"
                        name="./lastNameReq"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                        </options>
                    </lastNameValidation>
