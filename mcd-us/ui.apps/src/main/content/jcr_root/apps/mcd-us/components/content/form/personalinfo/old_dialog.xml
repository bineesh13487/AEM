                        jcr:primaryType="cq:Widget"
                        fieldDescription="Validation message for last name field. Default set to &lt;b>Please enter your Last Name&lt;/b>"
                        fieldLabel="Last Name Validation Message"
                        name="./lastNameMessage"
                        xtype="textfield"/>
                    <address
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text for address field. Default set to &lt;b>Address (US Only) *&lt;/b>"
                        fieldLabel="Address Text"
                        name="./address"
                        xtype="textfield"/>
                    <addressAriaLabel
                        jcr:primaryType="cq:Widget"                        
                        fieldLabel="Aria Label for address"
                        name="./addressAriaLabel"
                        xtype="textfield"/> 
                    <addressValidation
                        jcr:primaryType="cq:Widget"
                        defaultValue="yes"
                        fieldDescription="Please select address (us only) is required or not. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="Address Required"
                        name="./addressReq"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                        </options>
                    </addressValidation>
