                        fieldDescription="Please provide state name by clicking + button."
                        fieldLabel="State"
                        name="./state"
                        xtype="multifield">
                        <fieldConfig
                            jcr:primaryType="nt:unstructured"
                            xtype="textfield"/>
                    </state>
                    <stateValidation
                        jcr:primaryType="cq:Widget"
                        defaultValue="yes"
                        fieldDescription="Please select state is required or not. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="State Required"
                        name="./stateReq"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                        </options>
                    </stateValidation>
