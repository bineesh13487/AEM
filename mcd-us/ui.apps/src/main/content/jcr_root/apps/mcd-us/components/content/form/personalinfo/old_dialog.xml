                        jcr:primaryType="cq:Widget"
                        defaultValue="yes"
                        fieldDescription="Please select city is required or not. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="City Required"
                        name="./cityReq"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                        </options>
