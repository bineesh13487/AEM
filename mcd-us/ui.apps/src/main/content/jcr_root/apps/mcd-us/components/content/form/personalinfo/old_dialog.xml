                        jcr:primaryType="cq:Widget"
                        fieldDescription="Validation message for city field. Default set to &lt;b>Please enter your City&lt;/b>"
                        fieldLabel="City Validation Message"
                        name="./cityMessage"
                        xtype="textfield"/>
