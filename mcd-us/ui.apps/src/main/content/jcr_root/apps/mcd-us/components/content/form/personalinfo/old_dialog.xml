                        fieldDescription="Radio button yes text to be displayed for specific question. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="Yes Text"
                        name="./yesText"
                        xtype="textfield"/>
                    <hearBackNo
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text for hear back no field.Default set to &lt;b>No (Please know there may be instances where we need to contact you for additional information regarding this matter)&lt;/b>"
                        fieldLabel="Hear Back No Text"
                        name="./hearBackNo"
                        xtype="textfield"/>
                    <noText
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Radio button no text to be displayed for specific question. Default set to &lt;b>No&lt;/b>"
                        fieldLabel="No Text"
                        name="./noText"
                        xtype="textfield"/>
