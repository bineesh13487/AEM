                        jcr:primaryType="cq:Widget"
                        fieldDescription="Validation message for address field. Default set to &lt;b>Please enter your Address (U.S. Only)&lt;/b>"
                        fieldLabel="Address Validation Message"
                        name="./addressMessage"
                        xtype="textfield"/>
                    <apt
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text for apt/suite field. Default set to &lt;b>Apt/Suite&lt;/b>"
                        fieldLabel="Apt/Suite Text"
                        name="./apt"
                        xtype="textfield"/>
                    <aptAriaLabel
                        jcr:primaryType="cq:Widget"                        
                        fieldLabel="Aria Label for apt/suite"
                        name="./aptAriaLabel"
                        xtype="textfield"/> 
