                        jcr:primaryType="cq:Widget"
                        defaultValue="yes"
                        fieldDescription="Please select first name is required or not. Default set to &lt;b>Yes&lt;/b>"
                        fieldLabel="First Name Required"
                        name="./firstNameReq"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Yes"
                                value="yes"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="No"
                                value="no"/>
                        </options>
                    </firstNameValidation>
                    <firstNameValidationMessage
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Validation message for first name field. Default set to &lt;b>Please enter your First Name&lt;/b>"
                        fieldLabel="First Name Validation Message"
                        name="./firstNameMessage"
                        xtype="textfield"/>
                    <lastName
                        jcr:primaryType="cq:Widget"
                        fieldDescription="Text for last name field. Default set to &lt;b>Last Name *&lt;/b>"
                        fieldLabel="Last Name Text"
                        name="./lastName"
                        xtype="textfield"/>
                    <lastNameAriaLabel
                        jcr:primaryType="cq:Widget"                        
                        fieldLabel="Aria Label for last name"
                        name="./lnameAriaLabel"
                        xtype="textfield"/> 
