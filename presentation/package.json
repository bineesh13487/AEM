{
  "name": "mcdonalds-html-winter-release",
  "version": "1.0.0",
  "description": "McDonalds HTML Project",
  "main": "index.js",
  "scripts": {
    "open:dev": "opener http://localhost:4000",
    "dev": "nodemon -e ejs,js,css",
    "clean-dist": "node ./bin/clean-dist.js",
    "postinstall": "node ./bin/post-install-message.js",
    "start": "npm run webpack",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "./node_modules/.bin/eslint src --ext .js --ignore-path .eslintignore",
    "fix:js:dryrun": "./node_modules/.bin/eslint --fix-dry-run src --ext .js",
    "fix:js": "./node_modules/.bin/eslint --fix src --ext .js",
    "lint:css": "./node_modules/.bin/stylelint \"src/**/*.scss\"",
    "lint:bem": "./node_modules/.bin/bemlinter --config .bemlinterconfig",
    "fix:css": "./node_modules/.bin/stylelint \"src/**/*.scss\" --fix",
    "serve-build": "echo 'Serving distribution folder build/dist' && npm run serve-dist",
    "serve-dist": "./node_modules/.bin/serve build/dist",
    "build-lib": "clientlib --verbose",
    "build": "npm run clean-dist && npm run webpack-build",
    "build-prod": "npm run clean-dist && NODE_ENV=production npm run webpack-build-prod",
    "build-prod-all": "DEVTOOLS=false npm run build-prod && NODE_ENV=production OPTIMIZE=false DEVTOOLS=true DIST_DIR=dist/devtools npm run webpack-build",
    "webpack": "./node_modules/.bin/webpack-dev-server --progress --colors --hot --inline --host 0.0.0.0 --port 4000 --watch",
    "webpack-build": "./node_modules/.bin/webpack --progress",
    "webpack-build-prod": "./node_modules/.bin/webpack --progress -p",
    "local-webserver": "forever start -s ./node_modules/.bin/http-server build/dist/ -s",
    "test": "npm run test:validate && npm run test:unit && npm run test:e2e",
    "test:e2e": "npm run local-webserver & (wdio ./test-e2e/wdio.conf.js; wdio_ret=$?; forever stopall; exit $wdio_ret)",
    "test:unit": "jest --config=jest.json",
    "test:validate": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'"
  },
  "keywords": [],
  "copyright": "Sapient",
  "author": "Sapient for Capegemini",
  "license": "All parts Copyright.",
  "repository": {
    "type": "git",
    "url": ""
  },
  "pre-commit": [
    "lint"
  ],
  "indexTemplate": "homepage",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "chai": "4.1.2",
    "cssnano": "^3.10.0",
    "express": "^4.16.2",
    "express-partials": "^0.3.0",
    "jquery": "1.11",
    "wdio-cucumber-framework": "~1.0.2",
    "wdio-phantomjs-service": "~0.2.2",
    "wdio-selenium-standalone-service": "~0.0.9",
    "wdio-spec-reporter": "~0.1.2",
    "webdriverio": "^4.9.9"
  },
  "devDependencies": {
    "accessibility-developer-tools": "^2.12.0",
    "aem-clientlib-webpack-plugin": "^1.6.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "6.26.0",
    "bemlinter": "https://github.com/samunplugged/bemlinter.git#9c7f401",
    "caniuse-lite": "^1.0.30000697",
    "chai": "4.1.2",
    "css-loader": "^0.28.9",
    "del": "3.0.0",
    "ejs": "^2.5.7",
    "ejs-compiled-loader": "1.1.0",
    "ejs-webpack-builder": "^2.0.1",
    "eslint": "4.11.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-ignore-webpack": "^1.0.2",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "express": "^4.16.2",
    "express-partials": "^0.3.0",
    "extract-loader": "1.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "forever": "~0.15.3",
    "fs-extra": "^5.0.0",
    "git-rev-sync": "^1.4.0",
    "html-loader": "0.5.1",
    "htmlhint": "^0.9.13",
    "http-server": "~0.10.0",
    "jest": "21.2.1",
    "lodash.template": "4.4.0",
    "micromatch": "^3.1.5",
    "mocha": "4.0.1",
    "moment": "2.19.2",
    "my-local-ip": "1.0.0",
    "node-sass": "4.6.1",
    "node-sass-once-importer": "^5.1.0",
    "nodemon": "^1.12.1",
    "npmlog": "4.1.2",
    "opener": "^1.4.3",
    "pa11y": "^4.13.2",
    "path": "^0.12.7",
    "path-is-root": "^0.1.0",
    "patternengine-node-mustache": "^1.0.2",
    "phantomjs-bin": "2.1.1",
    "postcss": "6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "2.0.8",
    "postcss-sass": "^0.3.0",
    "postcss-scss": "^1.0.3",
    "pre-commit": "1.2.2",
    "precss": "^2.0.0",
    "ps-node": "^0.1.6",
    "reload": "^2.2.2",
    "rtlcss": "^2.2.1",
    "sass-loader": "6.0.6",
    "serve": "6.4.1",
    "sqlite3": "^3.1.13",
    "style-loader": "0.19.0",
    "stylelint": "8.3.0",
    "stylelint-config-standard": "18.0.0",
    "stylelint-webpack-plugin": "0.9.0",
    "sugarss": "1.0.1",
    "touch": "^3.1.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-rtl-plugin": "^1.6.0",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "private": true
}
